{
  "name": "@exoplay/exobot-adapter-discord",
  "version": "3.0.0",
  "description": "exobot adapter for discord",
  "main": "discord.js",
  "scripts": {
    "build": "exobot build -i src/discord.js -o discord.js",
    "watch": "exobot build --watch -i src/discord.js -o discord.js",
    "example": "exobot run -i example/exobot.config.js -o example/discord.example.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exoplay/exobot-adapter-discord.git"
  },
  "keywords": [
    "exobot",
    "chat",
    "bot",
    "discord"
  ],
  "author": "Jack Lawson <ajacksified@gmail.com>",
  "license": "LGPL",
  "bugs": {
    "url": "https://github.com/exoplay/exobot-adapter-discord/issues"
  },
  "homepage": "https://github.com/exoplay/exobot-adapter-discord#readme",
  "dependencies": {
    "discord.js": "^9.3.0"
  },
  "peerDependencies": {
    "@exoplay/exobot": ">= 4.1 < 5"
  },
  "devDependencies": {
    "@exoplay/exobot": "^4.1.3",
    "@exoplay/exobot-build": "^0.2.2",
    "regenerator-runtime": "^0.9.6"
  }
}
